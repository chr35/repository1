.btn{
	min-height: 32px;
	min-width: 100px;
	background-color: #337ab7;
	background-repeat: repeat;
	color: white;
	margin: 0px;
	border-width: 0px;
}
.btn:HOVER{
	outline-color:#338FCC;
	outline-style: solid;
}
.btn-intable{
	min-width: 32px;
	background-color:white;
	color: #656565;
	text-align: center;
	text-decoration: none;
	border-width:1px;
	border-style:solid;
	border-color:#dde6e9;
	white-space: nowrap;
	border-radius:4px;
}
.btn-intable:hover {background-color: #f5f5f5}
